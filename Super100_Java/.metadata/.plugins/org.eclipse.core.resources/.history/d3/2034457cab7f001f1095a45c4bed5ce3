package com;

import java.util.Stack;

public class Demo {

	public static void main(String[] args) {
		
		Stack sk = new Stack();
		
		//pushing elements into the stack
		
		sk.push(10);
		sk.push(30);
		sk.push(50);
		sk.push(40);
		sk.push(10);
		sk.push(90);
		sk.push(150);
		sk.push(410);
		sk.push(200);
		sk.push(12);
		sk.push(5);
		
		//peek -- return top of the element
		
		System.out.println("Fetch or Retrieve top of the Element :"+sk.peek());
		System.out.println("Initial Capacity of stack : "+sk.capacity());
		System.out.println("Size of stack :"+sk.size());
		System.out.println(sk.search(12));
		
		//search 
		
		for(int i=0;i<=sk.size()-1;i++)
		{
			
		}
}
}
