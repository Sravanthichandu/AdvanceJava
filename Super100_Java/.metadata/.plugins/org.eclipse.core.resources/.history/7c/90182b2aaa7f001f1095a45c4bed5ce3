package com;

import java.util.Stack;

public class Demo {

	public static void main(String[] args) {
		
		Stack sk = new Stack();
		
		//pushing elements into the stack
		
		sk.push(10);
		sk.push(20);
		sk.push(30);
		sk.push(40);
		sk.push(50);
		
		for(Object obj : sk)
		{
			System.out.println(obj);
		}
		
		while(!sk.isEmpty())
		{
			System.out.println(sk.pop());
		}
	   /*Stack stack = new Stack();
	   stack.push("Java");
	   stack.push("Python");
	   stack.push("C++");
	   stack.push(50);
	   
	   while(!stack.isEmpty())
	   {
		   System.out.println(stack.pop());
	   }*/
}
}
